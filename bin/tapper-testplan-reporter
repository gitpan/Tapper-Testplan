#! /usr/bin/perl
# PODNAME: tapper-testplan-reporter
# ABSTRACT: cmdline frontend to Tapper::Testplan::Reporter

use strict;
use warnings;
use 5.010;

use Tapper::Testplan::Reporter;
my $reporter = Tapper::Testplan::Reporter->new();
$reporter->run;

__END__
